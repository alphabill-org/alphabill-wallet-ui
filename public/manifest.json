{
  "short_name": "Alphabill Wallet",
  "name": "Alphabill Wallet",
  "description": "Alphabill Wallet",
  "version": "0.4.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html?popup=true",
    "default_title": "Open Alphabill Wallet"
