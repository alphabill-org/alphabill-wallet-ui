{
  "short_name": "Alphabill Wallet",
  "name": "Alphabill Wallet",
  "description": "Alphabill Wallet",
  "version": "0.1.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "*://*.dev-ab-wallet-backend.abdev1.guardtime.com/*",
    "*://*.dev-ab-faucet-api.abdev1.guardtime.com/*",
    "*://*.dev-ab-money-partition.abdev1.guardtime.com/*",
    "*://*.money-partition.testnet.alphabill.org/*",
    "*://*.wallet-backend.testnet.alphabill.org/*",
    "*://*.faucet.testnet.alphabill.org/*"
  ]
}